msgstr "    * 9 - privilegeWithdrawn\n"

#: ipaserver/plugins/cert.py:151
msgid "    * 10 - aACompromise\n"
msgstr "    * 10 - aACompromise\n"

#: ipaserver/plugins/cert.py:152
msgid ""
